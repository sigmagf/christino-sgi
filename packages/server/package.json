{
  "name": "@christino-sgi/server",
  "version": "0.1.0",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "prestart": "yarn build",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "build": "rimraf ./dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "prisma:migrate": "npm-run-all prisma:migrate:save prisma:migrate:up prisma:generate",
    "prisma:migrate:save": "prisma migrate save --experimental",
    "prisma:migrate:up": "prisma migrate up --experimental",
    "prisma:generate": "prisma generate",
    "prisma:ui": "prisma studio --experimental"
  },
  "prisma": {},
  "dependencies": {
    "@prisma/client": "^2.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@prisma/cli": "^2.11.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/morgan": "^1.9.1",
    "@types/uuid": "^8.3.0",
    "babel-plugin-root-import": "^6.5.0",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.51"
  },
  "license": "MIT"
}
